FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "constant";
    object      Kv;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 1.17982e-12;

boundaryField
{
    bc0
    {
        type            zeroGradient;
    }
    bc1
    {
        type            zeroGradient;
    }
    bc2
    {
        type            zeroGradient;
    }
    bc3
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
}


