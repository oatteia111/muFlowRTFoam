FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "constant";
    object      n_vg;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{
    bc0
    {
        type            calculated;
        value           uniform 1.5;
    }
    bc1
    {
        type            calculated;
        value           uniform 1.5;
    }
    bc2
    {
        type            calculated;
        value           uniform 1.5;
    }
    bc3
    {
        type            calculated;
        value           uniform 1.5;
    }
    top
    {
        type            calculated;
        value           uniform 1.5;
    }
    bottom
    {
        type            calculated;
        value           uniform 1.5;
    }
}


