FoamFile
{
 version 2.0;
 format ascii;
 class dictionary;
 location "system";
 object fvSchemes;
}

ddtSchemes
{
  default Euler;
}
gradSchemes
{
  default Gauss linear;
}
divSchemes
{
  default none;
  div(phiw,Cw) Gauss vanLeer;
  div(phiw,Cwi) Gauss vanLeer;
  div(phig,Cg) Gauss vanLeer;
  div(phig,Cgi) Gauss vanLeer;
}
laplacianSchemes
{
  default Gauss linear corrected;
}
interpolationSchemes
{
  default linear;
  M vanLeer phiw;
  krg upwind phig;
  krw upwind phiw;
}
snGradSchemes
{
  default corrected;
}
fluxRequired
{
  default no;
  h  ;
  hp  ;
  p  ;
}